   "source": [
    "SELECT\n",
    "  p.Name        AS ProductName,\n",
    "  p.ListPrice,\n",
    "  pc.Name       AS CategoryName\n",
    "FROM SalesLT.Product AS p\n",
    "LEFT JOIN SalesLT.ProductCategory AS pc\n",
    "  ON pc.ProductCategoryID = p.ProductCategoryID\n",
    "ORDER BY pc.Name, p.Name;\n"
   ]
