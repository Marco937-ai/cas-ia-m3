   "source": [
    "SELECT\n",
    "  c.CustomerID,\n",
    "  SUM(soh.TotalDue) AS TotalRevenue\n",
    "FROM SalesLT.Customer c\n",
    "JOIN SalesLT.SalesOrderHeader soh\n",
    "  ON soh.CustomerID = c.CustomerID\n",
    "GROUP BY c.CustomerID\n",
    "ORDER BY TotalRevenue DESC\n",
    "LIMIT 3;"
   ]
  }
