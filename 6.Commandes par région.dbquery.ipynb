   "source": [
    "SELECT\n",
    "  a.StateProvince,\n",
    "  COUNT(soh.SalesOrderID) AS TotalOrders\n",
    "FROM SalesLT.SalesOrderHeader AS soh\n",
    "JOIN SalesLT.Address AS a\n",
    "  ON a.AddressID = soh.ShipToAddressID\n",
    "GROUP BY a.StateProvince\n",
    "ORDER BY a.StateProvince;"
