   "source": [
    "SELECT\n",
    "  c.CustomerID,\n",
    "  COUNT(soh.SalesOrderID) AS TotalOrders\n",
    "FROM SalesLT.Customer AS c\n",
    "LEFT JOIN SalesLT.SalesOrderHeader AS soh\n",
    "  ON soh.CustomerID = c.CustomerID\n",
    "GROUP BY c.CustomerID\n",
    "ORDER BY TotalOrders DESC, c.CustomerID;"
   ]
  }
